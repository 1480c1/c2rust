---
test.c:
    - item: function
      name: main
      #disable_xchecks: true
      #entry: disabled
      #entry: { djb2: "" }

    - item: function
      name: foo
      disable_xchecks: true

    - item: function
      name: id
      disable_xchecks: true

    - item: function
      name: my_Foo_hash
      disable_xchecks: true

    - item: function
      name: fibo
      #disable_xchecks: true
      entry: { fixed: 0x12345678 }
      #entry: disabled
      #entry: { custom: "foo" }
      #all_args: { fixed: 0xabcdef00 }
      args:
        n: default
        #p: default
        #q: default
        foo: default
        #n: disabled
        #n: { custom: "id" }
        #n: { fixed: 0xabcd1234 }

    - item: struct
      name: Foo
      #custom_hash: "my_Foo_hash"
      #field_hasher: "foo_hasher"
...
