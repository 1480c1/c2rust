---
test.c:
    - item: defaults
      disable_xchecks: true

    - item: function
      name: main
      disable_xchecks: false
      #entry: disabled
      #entry: { djb2: "" }

    - item: function
      name: fibo
      disable_xchecks: false
      entry: { fixed: 0x12345678 }
      #entry: disabled
      #entry: { custom: "foo" }
      #all_args: { fixed: 0xabcdef00 }
      args:
        n: default
        #p: default
        #q: default
        foo: default
        #n: disabled
        #n: { custom: "id" }
        #n: { fixed: 0xabcd1234 }

    - item: struct
      name: Foo
      #custom_hash: "my_Foo_hash"
      #field_hasher: "foo_hasher"
      fields:
        #n1: disabled
        n1: { fixed: 0x1234 }
        #n2: disabled
        #n2: { fixed: 0x1234 }
        #n2: { custom: "id" }
        n2: { custom: "foo" }
...
